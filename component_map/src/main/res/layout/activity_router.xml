<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fenghuang.commonent_map.RouterActivity">


    <com.amap.api.maps.MapView
        android:id="@+id/router_map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/start_router"
        android:layout_marginBottom="@dimen/basic_d20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:paddingLeft="@dimen/basic_d20"
        android:paddingRight="@dimen/basic_d20"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/basic_white1"
        android:background="@drawable/bg_start_fench"
        android:paddingTop="@dimen/basic_d10"
        android:paddingBottom="@dimen/basic_d10"
        android:layout_height="wrap_content"/>

</android.support.constraint.ConstraintLayout>
